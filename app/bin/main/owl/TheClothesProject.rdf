    <!-- https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Accessory -->

    <owl:Class rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Accessory">
        <rdfs:subClassOf rdf:resource="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Clothes"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#HasSize"/>
                <owl:someValuesFrom rdf:resource="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#AccessorySize"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Target"/>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Bag"/>
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Belt"/>
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Hat"/>
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Jewellery"/>
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Scarf"/>
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Sunglasses"/>
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Tie"/>
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Umbrella"/>
        </owl:disjointUnionOf>
    </owl:Class>
    



    <owl:Class rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#CharacteristicOfClothes">
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#ClothesMaterial"/>

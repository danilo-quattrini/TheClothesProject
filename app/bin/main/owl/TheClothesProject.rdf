    <!-- https://www.unicam.it/cs/daniloquattrini/TheClothesProject#ChildrenClothes -->

    <owl:Class rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#ChildrenClothes">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#Target"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.unicam.it/cs/daniloquattrini/TheClothesProject#hasAge"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</xsd:minInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</xsd:maxInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
